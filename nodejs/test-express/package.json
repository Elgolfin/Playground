{
  "name": "test-cookies",
  "version": "1.0.0",
  "description": "Sample node project to play with cookies",
  "scripts": {
    "start": "pm2 kill && npm run build && pm2 start dist/server.js --name=test-cookies",
    "launch-browser": "node launch-browser.js",
    "startdev": "pm2 kill && pm2 start dist/server.js --watch --name=test-cookies && npm run launch-browser",
    "restart": "pm2 restart test-cookies",
    "stop": "pm2 delete test-cookies",
    "lint": "eslint --ext .js, src test/unit/specs",
    "test": "pm2 kill && istanbul cover node_modules/mocha/bin/_mocha test/unit/specs -- --compilers js:babel-register",
    "testwatch": "mocha test/unit/specs --compilers js:babel-register -w",
    "build": "babel src -d dist --presets es2015,stage-2",
    "serve": "npm run build && npm run startdev"
  },
  "dependencies": {
    "auth0-js": "^8.1.3",
    "axios": "^0.15.3",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "cookies": "^0.6.2",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "http-proxy-middleware": "^0.17.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "pm2": "^2.3.0",
    "request": "^2.79.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "winston": "^2.3.0",
    "winston-papertrail": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul-reporter-shield-badge": "^1.1.3",
    "opn": "^4.0.2",
    "sinon-stub-promise": "^4.0.0"
  }
}
